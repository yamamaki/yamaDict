{
  "manifest_version": 2,
  "name": "yamaDict",
  "description": "",
  "version": "1.0",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "",
    "default_popup": "html/popup.html"
  },
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "background": {"scripts": ["js/jquery-1.8.3.min.js", "js/background.js"]},
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["js/jquery-1.8.3.min.js", "js/yamaDict.js"]
    }
  ],
  "permissions": [
    "tabs",
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ]
}

